    "@testing-library/react": "^14.0.0",
    "framer-motion": "^10.12.4",
    "luxon": "^3.3.0",
    "rc-scrollbars": "^1.1.5",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "react-scrollbars-custom": "^4.1.1",
    "react-select": "^5.7.3",
    "react-use-websocket": "^4.3.1",
    "react-verification-code-input": "^1.2.9",
    "web-vitals": "^3.3.1",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --watchAll=false",
    "test-w-coverage": "craco test --coverage --watchAll=false",
    "format": "npx prettier --write src public && npx stylelint \"**/*.css\" --fix",
    "lint": "npx prettier --check src public && npx stylelint \"**/*.css\"",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@chakra-ui/storybook-addon": "^4.0.16",
    "@craco/craco": "^7.1.0",
    "@redux-devtools/core": "^3.13.1",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/user-event": "^14.4.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "msw": "^1.2.1",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "storybook": "^7.0.6",
    "storybook-addon-mock": "^4.0.0",
    "stylelint": "^15.6.1",
    "stylelint-order": "^6.0.3",
    "webpack": "^5.80.0"
  }
}